<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    
    <!-- アプリケーション全般 -->
    <system:String x:Key="AppTitle">Teams アカウントマネージャー</system:String>
    <system:String x:Key="Language">言語</system:String>
    <system:String x:Key="Ready">準備完了</system:String>
    <system:String x:Key="Loading">読み込み中...</system:String>
    <system:String x:Key="Error">エラー</system:String>
    <system:String x:Key="Warning">警告</system:String>
    <system:String x:Key="Information">情報</system:String>
    <system:String x:Key="Confirm">確認</system:String>
    <system:String x:Key="Yes">はい</system:String>
    <system:String x:Key="No">いいえ</system:String>
    <system:String x:Key="OK">OK</system:String>
    <system:String x:Key="Cancel">キャンセル</system:String>
    <system:String x:Key="Save">保存</system:String>
    <system:String x:Key="Close">閉じる</system:String>
    
    <!-- 認証関連 -->
    <system:String x:Key="Login">ログイン</system:String>
    <system:String x:Key="Logout">ログアウト</system:String>
    <system:String x:Key="LoginMessage">Microsoft アカウントでログインしてください</system:String>
    <system:String x:Key="LoginSuccess">ログインに成功しました</system:String>
    <system:String x:Key="LoginFailed">ログインに失敗しました</system:String>
    <system:String x:Key="LogoutConfirm">ログアウトしてもよろしいですか？</system:String>
    
    <!-- ユーザー管理 -->
    <system:String x:Key="Users">ユーザー</system:String>
    <system:String x:Key="UserList">ユーザー一覧</system:String>
    <system:String x:Key="DisplayName">表示名</system:String>
    <system:String x:Key="Email">メールアドレス</system:String>
    <system:String x:Key="Department">部署</system:String>
    <system:String x:Key="JobTitle">役職</system:String>
    <system:String x:Key="OfficeLocation">オフィス所在地</system:String>
    <system:String x:Key="PhoneNumber">電話番号</system:String>
    <system:String x:Key="AccountEnabled">アカウント有効</system:String>
    <system:String x:Key="LastSignIn">最終ログイン</system:String>
    
    <!-- 検索・フィルター -->
    <system:String x:Key="Search">検索</system:String>
    <system:String x:Key="SearchPlaceholder">名前またはメールアドレスで検索...</system:String>
    <system:String x:Key="Filter">フィルター</system:String>
    <system:String x:Key="FilterByDepartment">部署でフィルター</system:String>
    <system:String x:Key="FilterByStatus">ステータスでフィルター</system:String>
    <system:String x:Key="ClearFilter">フィルターをクリア</system:String>
    <system:String x:Key="ShowActive">有効なユーザーのみ</system:String>
    <system:String x:Key="ShowInactive">無効なユーザーのみ</system:String>
    <system:String x:Key="ShowAll">すべて表示</system:String>
    
    <!-- データ操作 -->
    <system:String x:Key="Import">インポート</system:String>
    <system:String x:Key="Export">エクスポート</system:String>
    <system:String x:Key="ExportToExcel">Excelにエクスポート</system:String>
    <system:String x:Key="ExportToCsv">CSVにエクスポート</system:String>
    <system:String x:Key="ImportFromExcel">Excelからインポート</system:String>
    <system:String x:Key="ImportFromCsv">CSVからインポート</system:String>
    <system:String x:Key="SelectFile">ファイルを選択</system:String>
    
    <!-- 一括編集 -->
    <system:String x:Key="BulkEdit">一括編集</system:String>
    <system:String x:Key="BulkEditTooltip">選択したユーザーを一括編集</system:String>
    <system:String x:Key="SelectedUsers">選択したユーザー</system:String>
    <system:String x:Key="Apply">適用</system:String>
    <system:String x:Key="Enabled">有効</system:String>
    <system:String x:Key="Disabled">無効</system:String>
    
    <!-- 変更保存 -->
    <system:String x:Key="SaveChanges">変更を保存</system:String>
    <system:String x:Key="SaveChangesTooltip">変更されたユーザー情報を保存</system:String>
    
    <!-- 変更履歴 -->
    <system:String x:Key="ChangeHistory">変更履歴</system:String>
    <system:String x:Key="User">ユーザー</system:String>
    <system:String x:Key="Property">プロパティ</system:String>
    <system:String x:Key="OriginalValue">元の値</system:String>
    <system:String x:Key="NewValue">新しい値</system:String>
    <system:String x:Key="ChangedAt">変更日時</system:String>
    <system:String x:Key="Status">ステータス</system:String>
    <system:String x:Key="Action">アクション</system:String>
    <system:String x:Key="Revert">元に戻す</system:String>
    <system:String x:Key="ClearHistory">履歴をクリア</system:String>
    <system:String x:Key="Close">閉じる</system:String>
    <system:String x:Key="ExportSuccess">エクスポートが完了しました</system:String>
    <system:String x:Key="ImportSuccess">インポートが完了しました</system:String>
    <system:String x:Key="ExportFailed">エクスポートに失敗しました</system:String>
    <system:String x:Key="ImportFailed">インポートに失敗しました</system:String>
    
    <!-- 編集 -->
    <system:String x:Key="Edit">編集</system:String>
    <system:String x:Key="BatchEdit">一括編集</system:String>
    <system:String x:Key="Update">更新</system:String>
    <system:String x:Key="UpdateSuccess">更新が完了しました</system:String>
    <system:String x:Key="UpdateFailed">更新に失敗しました</system:String>
    <system:String x:Key="NoChanges">変更はありません</system:String>
    <system:String x:Key="ConfirmUpdate">以下の変更を適用しますか？</system:String>
    <system:String x:Key="SelectUsers">ユーザーを選択してください</system:String>
    
    <!-- 統計・レポート -->
    <system:String x:Key="Statistics">統計</system:String>
    <system:String x:Key="TotalUsers">総ユーザー数</system:String>
    <system:String x:Key="ActiveUsers">有効ユーザー数</system:String>
    <system:String x:Key="InactiveUsers">無効ユーザー数</system:String>
    <system:String x:Key="UsersByDepartment">部署別ユーザー数</system:String>
    
    <!-- エラーメッセージ -->
    <system:String x:Key="NetworkError">ネットワークエラーが発生しました</system:String>
    <system:String x:Key="AuthenticationError">認証エラーが発生しました</system:String>
    <system:String x:Key="PermissionError">権限がありません</system:String>
    <system:String x:Key="UnknownError">不明なエラーが発生しました</system:String>
    <system:String x:Key="RetryMessage">もう一度お試しください</system:String>
    
    <!-- ファイル形式 -->
    <system:String x:Key="ExcelFiles">Excel ファイル</system:String>
    <system:String x:Key="CsvFiles">CSV ファイル</system:String>
    <system:String x:Key="AllFiles">すべてのファイル</system:String>
</ResourceDictionary>