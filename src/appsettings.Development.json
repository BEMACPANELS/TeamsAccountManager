{
  "AzureAd": {
    "TenantId": "YOUR_DEV_TENANT_ID_HERE",
    "ClientId": "YOUR_DEV_CLIENT_ID_HERE",
    "RedirectUri": "http://localhost:8080"
  },
  "Application": {
    "PageSize": 50,
    "TimeoutSeconds": 60,
    "DefaultExportPath": "dev-exports",
    "DefaultImportPath": "dev-imports",
    "LogPath": "dev-logs"
  },
  "MCP": {
    "MicrosoftLearn": {
      "CacheEnabled": false,
      "TimeoutSeconds": 30
    },
    "Serena": {
      "Endpoint": "YOUR_DEV_SERENA_ENDPOINT_HERE",
      "TimeoutSeconds": 30
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Information",
      "TeamsAccountManager": "Trace"
    }
  }
}