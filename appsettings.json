{
  "AzureAd": {
    "TenantId": "YOUR_TENANT_ID_HERE",
    "ClientId": "YOUR_CLIENT_ID_HERE",
    "RedirectUri": "http://localhost",
    "Scopes": [
      "User.Read.All",
      "User.ReadWrite.All",
      "Directory.Read.All"
    ]
  },
  "Application": {
    "DefaultLanguage": "ja-JP",
    "PageSize": 100,
    "TimeoutSeconds": 30,
    "RetryCount": 3,
    "DefaultExportPath": "exports",
    "DefaultImportPath": "imports",
    "LogPath": "logs"
  },
  "MCP": {
    "MicrosoftLearn": {
      "Endpoint": "https://learn.microsoft.com/api/mcp",
      "Enabled": true,
      "TimeoutSeconds": 10,
      "CacheEnabled": true,
      "CacheDurationMinutes": 60
    },
    "Serena": {
      "Endpoint": "YOUR_SERENA_ENDPOINT_HERE",
      "Enabled": true,
      "TimeoutSeconds": 15,
      "ApiKey": "YOUR_SERENA_API_KEY_HERE"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "TeamsAccountManager": "Debug"
    }
  }
}